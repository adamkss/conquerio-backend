{
    "family": "quizio-nestjs",
    "executionRoleArn": "arn:aws:iam::026079580296:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "quizio-nestjs",
            "image": "026079580296.dkr.ecr.us-east-2.amazonaws.com/quizio-nestjs:latest",
            "portMappings": [
                {
                    "containerPort": 4000,
                    "hostPort": 4000,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "awslogs-quizio-backend",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "awslogs-quizio-backend"
                }
            },
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}